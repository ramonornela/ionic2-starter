{
  "name": "ionic2-starter",
  "version": "0.0.1",
  "description": "Projeto starter com features comuns para os projetos.",
  "private": true,
  "scripts": {
    "build:android": "cordova prepare --platforms android && npm run ionic:build",
    "postbuild:android": "cordova build android",
    "build:ios": "cordova prepare --platforms ios && npm run ionic:build",
    "postbuild:ios": "cordova build ios",
    "build:wp": "cordova prepare --platforms windows && npm run ionic:build",
    "postbuild:wp": "cordova build windows",
    "build:ci": "npm run clean:cordova && cordova prepare --platforms && ionic build android ios",
    "clean:cordova": "rimraf ./platforms/* ./plugins/*",
    "copy": "node ./scripts/app/copy",
    "commit": "git-cz",
    "commitmsg": "node node_modules/commitplease",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "gh:release": "node ./scripts/git/release",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "lint": "ionic-app-scripts lint",
    "postinstall": "node ./scripts/app/create.env",
    "preionic:build": "node ./scripts/app/replace.env",
    "preionic:serve": "node ./scripts/app/replace.env",
    "prepush": "npm run lint --bailOnLintError true",
    "server:init": "json-server ./server/router.js --watch"
  },
  "config": {
    "ionic_copy": "./scripts/app/copy.config.js",
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@ionic-native/core": "4.0.0",
    "@ionic-native/splash-screen": "4.0.0",
    "@ionic-native/status-bar": "4.0.0",
    "@ionic/storage": "2.0.1",
    "@mbamobi/configuration": "3.0.0",
    "@mbamobi/http": "3.0.0",
    "@mbamobi/http-plugins-ionic": "3.0.0",
    "@mbamobi/url-resolver": "3.0.0",
    "@ramonornela/json-server": "0.9.4",
    "angular2-text-mask": "8.0.1",
    "intl": "1.2.5",
    "ionic-angular": "3.5.3",
    "ionicons": "3.0.0",
    "rxjs": "5.4.1",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@ionic/app-scripts": "2.0.2",
    "codelyzer": "3.0.1",
    "commitizen": "^2.8.6",
    "commitplease": "2.6.1",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "github": "0.2.4",
    "husky": "^0.11.9",
    "rimraf": "2.5.4",
    "shelljs": "0.7.6",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "4.0.0",
    "typescript": "^2.4.1"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore"
    ],
    "scope": "\\S+.*"
  }
}
